package Day8;

public class constructor {
	int age;
	String name;
	constructor(){
	    System.out.println("Hi");
	}
	constructor(int a,String str){
		age=a;
		name=str;
		
	}
	void display() {
		System.out.println(age+" "+name);
	}

	public static void main(String[] args) {
		constructor obj =new constructor(19,"Aakash");
		

	}

}
